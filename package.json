{
  "name": "typeguard",
  "version": "0.0.7",
  "description": "TypeScript TypeGuards",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "compile": "tsc --project tsconfig.json",
    "compile:watch": "tsc --project tsconfig.json --watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist/**",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/densebrain/typeguard.git"
  },
  "keywords": [
    "typescript",
    "guard",
    "typeguard",
    "typesafe",
    "type",
    "check"
  ],
  "author": "Jonathan Glanz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/densebrain/typeguard/issues"
  },
  "jest": {
    "verbose": true,
    "scriptPreprocessor": "./node_modules/ts-jest/preprocessor.js",
    "testRegex": "src\\/.*\\.spec\\.(ts|tsx)$",
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "homepage": "https://github.com/densebrain/typeguard#readme",
  "dependencies": {
    "immutable": "^3.8.1",
    "lodash": "^4.16.6"
  },
  "devDependencies": {
    "@types/immutable": "^3.8.4",
    "@types/jest": "^15.1.32",
    "@types/lodash": "^4.14.38",
    "@types/node": "^6.0.46",
    "jest": "^16.0.2",
    "ts-jest": "0.1.8",
    "typescript": "^2.1.0-dev.20161031"
  }
}
